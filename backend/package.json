{
  "name": "websocket_reservations",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "watch": {
    "devstart": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": false,
      "legacyWatch": true,
      "delay": 1500
    }
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "copyassets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all lint tsc",
    "start": "node .",
    "devstart": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec npm run devstart",
    "watch_dev": "npm-watch devstart",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "npm-watch": "^0.11.0",
    "redis": "^3.1.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.7.6",
    "@types/redis": "^2.8.31",
    "@types/shelljs": "^0.8.9",
    "fs-extra": "^10.0.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2"
  }
